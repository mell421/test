#!/usr/bin/env bash

echo "2016 =";
./recherche_serie 2016 | wc -l ;
echo "";
echo "2017 =";
./recherche_serie 2017 | wc -l ;
echo "";
echo "2018 =";
./recherche_serie 2018 | wc -l ;
echo "";
echo "2016 commencées =";
./recherche_serie 2016 101 | wc -l ;
echo "";
echo "2017 commencées =";
./recherche_serie 2017 101 | wc -l ;
echo "";
echo "2018 commencées =";
./recherche_serie 2018 101| wc -l ;
echo "";
echo "";
echo "2017 terminées =";
./recherche_serie 2017 _ | wc -l ;
echo "";
echo "2018 terminées =";
./recherche_serie 2018 _ | wc -l ;
echo "";
echo "commencées =";
./recherche_serie 101| wc -l ;
echo "";
echo "terminées le jour meme =";
./recherche_serie _ 101| wc -l ;
echo "";
